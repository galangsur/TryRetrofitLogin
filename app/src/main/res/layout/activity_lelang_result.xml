<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/green1"
    tools:context=".activity.LelangResultActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/RL_lelres"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/reqlelasset_a">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/winnerflat" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rellayPemenang"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:padding="15dp"
                android:layout_below="@id/RL_lelres"
                android:background="@drawable/detaillelasset_a">
                <TextView
                    android:id="@+id/txt_result1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="Nama Pemenang :"
                    android:textColor="@color/green1"
                    android:textStyle="bold"
                    android:textSize="16dp"/>
                <EditText
                    android:id="@+id/rslt_namapemenang"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="none"
                    android:layout_below="@+id/txt_result1"/>
                <TextView
                    android:id="@+id/txt_result2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="Harga Akhir Yang Dimenangkan :"
                    android:textColor="@color/green1"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:layout_below="@id/rslt_namapemenang"/>
                <EditText
                    android:id="@+id/rslt_nilaiakhir"
                    android:layout_width="292dp"
                    android:layout_height="wrap_content"
                    android:inputType="none"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/txt_result2"/>
                <ImageView
                    android:id="@+id/smallwalleticoncpng"
                    android:layout_width="31dp"
                    android:layout_height="31dp"
                    android:background="@drawable/coinrp"
                    android:layout_below="@id/txt_result2"
                    android:layout_toLeftOf="@id/rslt_nilaiakhir"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"/>

                <TextView
                    android:id="@+id/txt_result8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="Harga Awal Lelang :"
                    android:layout_below="@id/rslt_nilaiakhir"
                    android:textColor="@color/green1"
                    android:textStyle="bold"
                    android:textSize="16dp"/>
                <EditText
                    android:id="@+id/rslt_hargaawal"
                    android:layout_width="292dp"
                    android:layout_height="wrap_content"
                    android:inputType="none"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/txt_result8"/>
                <ImageView
                    android:id="@+id/smallwalleticoncpng1"
                    android:layout_width="31dp"
                    android:layout_height="31dp"
                    android:background="@drawable/coinrp"
                    android:layout_below="@id/txt_result8"
                    android:layout_toLeftOf="@id/rslt_hargaawal"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"/>

                <Button
                    android:id="@+id/reslt_tohome"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="100dp"
                    android:text="ke menu"
                    android:layout_marginRight="50dp"
                    android:layout_marginLeft="50dp"
                    android:textColor="@color/white"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@id/rslt_hargaawal"
                    android:background="@drawable/btnasset"
                    android:layout_centerHorizontal="true"/>

                <Button
                    android:id="@+id/reslt_buattrans"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="100dp"
                    android:text="Buat Nota"
                    android:layout_marginRight="50dp"
                    android:layout_alignParentRight="true"
                    android:textColor="@color/white"
                    android:layout_below="@id/rslt_hargaawal"
                    android:background="@drawable/btnasset"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hasil Akhir Lelang"
                android:layout_gravity="center"
                android:layout_centerHorizontal="true"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:layout_marginTop="25dp" />

            <TextView
                android:id="@+id/reslelimgprnt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_messageId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_userId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/txtstatshasilawalid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/txtstatshasilawal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_hewanid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_userName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_pelelangId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_pesertaId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/lelbrjalankey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>

            <TextView
                android:id="@+id/reslt_pelelangName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>
            <TextView
                android:id="@+id/statusrsltkey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>
            <TextView
                android:id="@+id/statusrslt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>
            <TextView
                android:id="@+id/rsltidpelelang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>
            <TextView
                android:id="@+id/rsltidlelbrjalan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green1"/>


        </RelativeLayout>
    </ScrollView>

</RelativeLayout>