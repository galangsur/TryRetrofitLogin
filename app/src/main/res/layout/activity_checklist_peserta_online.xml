<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/green1"
    tools:context=".activity.ChecklistPesertaOnlineActivity">

    <ImageView
        android:id="@+id/btn_backchecklistpeserta"
        android:layout_width="25dp"
        android:layout_height="35dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="23dp"
        android:src="@drawable/ic_baseline_arrowwhite_24"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Peserta Lelang"
        android:layout_centerHorizontal="true"
        android:textSize="25sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_marginTop="25dp"
        android:textAlignment="center"/>

    <TextView
        android:id="@+id/tmp_idlelbrjalanchecklistpesertaonline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:textColor="@color/green1"/>

    <TextView
        android:id="@+id/tmppesertaonline_token"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:textColor="@color/green1"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="100dp">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/RV_checklistpesertaonline"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="vertical"
            android:background="@drawable/signasset_c"
            android:padding="10dp"/>
    </RelativeLayout>

</RelativeLayout>