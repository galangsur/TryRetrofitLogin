<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tryretrofitlogin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.DetailLelangActivity"></activity>
        <activity android:name=".activity.WalletCreateActivity" />
        <activity android:name=".activity.ListLelangActivity" />
        <activity android:name=".activity.AddLelangActivity" />
        <activity android:name=".activity.WalletActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.auth.SignupActivity" />
        <activity android:name=".activity.auth.SigninActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>